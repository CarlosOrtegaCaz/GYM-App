<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.gymmf">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="com.example.gymmf.FormularioPaypal"
            android:label="Pago con PayPal" />
        <activity
            android:name="com.example.gymmf.FormularioOxxo"
            android:label="Pago en OXXO" />
        <activity
            android:name="com.example.gymmf.FormularioCodigoGym"
            android:label="Código GYM" />
        <activity
            android:name="com.example.gymmf.FormularioPago"
            android:label="Pago con tarjeta de crédito o débito" />
        <activity
            android:name="com.example.gymmf.Exito"
            android:label="Registro correcto" />
        <activity
            android:name="com.example.gymmf.Splash"
            android:theme="@style/AppTheme">

        </activity>
        <activity
            android:name="com.example.gymmf.Inscripcion"
            android:label="Registro para la inscripción" />
        <activity
            android:name="com.example.gymmf.Metodo"
            android:label="Métodos de pago" />
        <activity
            android:name="com.example.gymmf.Planes"
            android:label="Planes" />
        <activity
            android:name="com.example.gymmf.Login"
            android:label="GYM2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.gymmf.MainActivity"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>